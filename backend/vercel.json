
{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/todo"
      },
      {
        "src": "/(.*)",
        "dest": "/todo/new"
      }, {
        "src": "/(.*)",
        "dest": "/todo/delete/:id"
      }

    ]
  }